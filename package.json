{
  "name": "pet-shop-inventory-management",
  "version": "1.0.0",
  "description": "Pet Shop Inventory Management System",
  "main": "server/index.js",
  "scripts": {
    "build": "cd client && npm run build",
    "build:move": "npm run build && Move-Item -Path \"client\\build\" -Destination \"server\\build\" -Force",
    "start": "cd server && npm start",
    "dev:client": "cd client && npm start",
    "dev:server": "cd server && npm run dev",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "keywords": ["pet-shop", "inventory", "management", "react", "nodejs"],
  "author": "",
  "license": "ISC"
} 